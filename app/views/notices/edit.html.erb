
<h1>공지 수정</h1>
<%= form_with model: @notice do |form| %>
<div class="field">
  <%= form.label :title %>
  <%= form.text_field :title%>
</div>

<div class="field">
  <%= form.label :content %>
  <%= form.text_field :content%>
</div>

<div class="actions">
  <%= form.submit %>
</div>
<% end %>

<%= link_to 'Show', @notice %> |
<%= link_to 'Back', notices_path %>
