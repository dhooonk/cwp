
<h1>프로그램 수정</h1>
<%= form_with model: @program do |form| %>
<div class="field">
  <%= form.label :programName %>
  <%= form.text_field :programName%>
</div>

<div class="field">
  <%= form.label :programStartDay %>
  <%= form.text_field :programStartDay%>
</div>
<div class="field">
  <%= form.label :programEndDay %>
  <%= form.text_field :programEndDay%>
</div>
<div class="field">
  <%= form.label :programDoingDay %>
  <%= form.text_field :programDoingDay%>
</div>
<div class="field">
  <%= form.label :programDoingTime %>
  <%= form.text_field :programDoingTime%>
</div>

<div class="actions">
  <%= form.submit %>
</div>
<% end %>

<%= link_to 'Show', @program %> |
<%= link_to 'Back', programs_path %>
